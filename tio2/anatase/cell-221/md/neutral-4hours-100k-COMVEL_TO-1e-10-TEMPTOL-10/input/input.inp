 &GLOBAL
   PROJECT_NAME tio2
   RUN_TYPE MD
   WALLTIME 23:55:00
 &END GLOBAL
 &MOTION
   &MD
     ENSEMBLE NVE
     COMVEL_TOL 1e-10
     TEMP_TOL 10
     STEPS 50000
     TIMESTEP  4.9999999999999989E-001
     TEMPERATURE  100
   &END MD
   &PRINT
     &TRAJECTORY SILENT
       FORMAT XMOL
     &END TRAJECTORY
     &VELOCITIES SILENT
       UNIT "angstrom*fs^-1"
       FORMAT XMOL
     &END VELOCITIES
     &FORCES SILENT
       UNIT "au_f"
       FORMAT XMOL
     &END FORCES
     &RESTART SILENT
       &EACH
         MD 1
       &END EACH
     &END RESTART
   &END PRINT
 &END MOTION 
&FORCE_EVAL
   METHOD  QS
   &DFT
     BASIS_SET_FILE_NAME BASIS_MOLOPT
     POTENTIAL_FILE_NAME GTH_POTENTIALS
     UKS  T
     CHARGE  0
     MULTIPLICITY 1
     &SCF
       MAX_SCF  40
       EPS_SCF     1.0000000000000003E-06
       SCF_GUESS  ATOMIC
       &OT  T
         MINIMIZER  DIIS
         PRECONDITIONER  FULL_SINGLE_INVERSE
       &END OT
       &OUTER_SCF  T
         EPS_SCF     1.0000000000000003E-06
         MAX_SCF  60
       &END OUTER_SCF
     &END SCF
     &QS
       EPS_DEFAULT     9.9999999999999998E-13
     &END QS
     &MGRID
       CUTOFF     6.0000000000000000E+02
     &END MGRID
     &XC
         &XC_FUNCTIONAL PBE
         &END XC_FUNCTIONAL     
	 &END XC
     &PRINT
       &E_DENSITY_CUBE  ON
       &EACH
            MD 100
         &END EACH
        STRIDE  1 1 1
       &END E_DENSITY_CUBE
    &MO_CUBES ON
     NHOMO 1
     NLUMO 1
     &EACH
            MD 100
         &END EACH
     &END MO_CUBES
    &HIRSHFELD
    SHAPE_FUNCTION  DENSITY
         ADD_LAST NUMERIC
         COMMON_ITERATION_LEVELS 0
         FILENAME hirshfeld/hirshfeld
         &EACH
            GEO_OPT 1
         &END EACH
         &END HIRSHFELD
         &END PRINT
   &END DFT
   
   &SUBSYS
    
      &CELL
ABC 7.56 7.56 9.62
ALPHA_BETA_GAMMA 90 90 90
SYMMETRY MONOCLINIC_GAMMA_AB
&END CELL
 
      &KIND Ti
         BASIS_SET SZV-MOLOPT-SR-GTH-q12
         POTENTIAL GTH-PBE-q12
       &DFT_PLUS_U on
        L 2
        U_MINUS_J [eV] 4.1
      &END DFT_PLUS_U
      &END KIND

    &KIND O
      ELEMENT O
      BASIS_SET DZVP-MOLOPT-SR-GTH-q6
      POTENTIAL GTH-PBE-q6
    &END KIND
     
     &TOPOLOGY
       COORD_FILE_NAME system.xyz
       COORD_FILE_FORMAT  XYZ
       MULTIPLE_UNIT_CELL  1 1 1
     &END TOPOLOGY
   &END SUBSYS
 &END FORCE_EVAL
